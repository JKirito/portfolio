    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run lint && npm run check-types"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.5",
    "framer-motion": "^6.2.7",
    "next": "12.1.0",
