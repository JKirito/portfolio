    "react-dom": "17.0.2",
    "sass": "^1.49.8"
  },
  "devDependencies": {
    "@types/node": "^17.0.19",
    "@types/react": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-react": "^7.28.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
